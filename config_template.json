{
  "generator_type": "pepmdlm",
  "base_generator_path": "pretrained/generators/peptune-pretrained.ckpt",
  "text_encoder_name": "Qwen/Qwen3-Embedding-0.6B",
  "preference_net_path": "pretrained/language/preference_net.ckpt",
  "dfm_model_path": "pretrained/generators/pepdfm.ckpt",
  "protein_seq": "",
  "predictors": {
    "binding": {
      "path": "pretrained/predictors/binding-affinity.pt",
      "type": "binding"
    },
    "toxicity": {
      "path": "pretrained/predictors/toxicity_predictor.pkl",
      "type": "toxicity"
    }
  },
  "constraints": {
    "weights": {
      "binding": 1.0,
      "toxicity": 1.0,
      "halflife": 0.5
    },
    "strength": 1.0
  },
  "training_prompts": [
    "Generate a peptide with high binding affinity and low toxicity",
    "Generate a stable peptide with long half-life",
    "Generate a balanced peptide optimizing binding, toxicity, and half-life",
    "Create a peptide that binds strongly to the target protein",
    "Design a peptide with minimal toxicity for therapeutic use",
    "Generate a peptide with extended half-life in serum"
  ],
  "experiment_settings": {
    "num_samples": 1000,
    "num_cycles": 1000,
    "trajectory_length": 50,
    "seed": 42
  }
}

